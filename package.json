  "scripts": {
    "build": "npm run clean && npm run compile && webpack --config webpack.config.js",
    "circle:coverage": "nyc --reporter=lcov --reporter=html npm test -- --reporter mocha-junit-reporter --reporter-options mochaFile=reports/mocha/test-results.xml",
    "circle:coveralls": "nyc report --reporter=text-lcov | coveralls",
    "circle:lint": "npm run lint -- --format junit --output-file ./reports/eslint/eslint.xml",
    "clean": "del .build/",
    "clear-cache-reference-data": "node tasks/clear-cache-reference-data.js",
    "compile": "tsc --project ./",
    "contributors": "(git-authors-cli && finepack && git add package.json && git commit -m 'build: contributors' --no-verify) || true",
    "coverage": "nyc --reporter=html --reporter=lcov npm test",
    "coverage:open": "npm run coverage && open-cli ./coverage/index.html",
    "develop": "run-p watch:js:*",
    "git:checkbranch": "CURRENT_BRANCH=$(git rev-parse --abbrev-ref HEAD); if [ \"$CURRENT_BRANCH\" = \"main\" ]; then echo 'Committing to the main branch is not allowed'; exit 1; fi",
    "lint": "eslint . --cache",
    "lint-staged": "lint-staged",
    "postinstall": "husky install",
    "postpublish": "pinst --enable",
    "postrelease": "npm run release:tags",
    "prepublishOnly": "pinst --disable",
    "prerelease": "npm run update:check && npm run contributors",
    "release": "standard-version --no-verify -a",
    "release:tags": "git push --follow-tags origin HEAD:main",
    "start": "node start.js",
    "test": "NODE_ENV=test LOG_LEVEL=silent npx ts-mocha './app/**/*.test.js' './common/**/*.test.js' './config/**/*.test.js'",
    "test-e2e": "npx ts-node test/e2e/parallel-runner.ts",
    "test-e2e:browserstack:ie": "testcafe 'browserstack:IE@11.0:Windows 10' test/e2e/*.test.js",
    "test-e2e:local": "E2E_BASE_URL='' npm run test-e2e -- --debug --reporter false",
    "test-e2e:local:api": "E2E_BASE_URL='' npm run test-e2e -- --reporter false --skip test/e2e/move.new.stc.test.js --skip test/e2e/move.update.stc.test.js",
    "update": "ncu --upgrade",
    "update:check": "ncu -- --error-level 2",
    "update:minor": "ncu --upgrade --target minor",
    "watch:js:client": "webpack --watch --progress",
    "watch:js:server": "nodemon $NODE_DEBUG_OPTION --inspect",
    "watch:js:typescript": "tsc --project ./ --watch",
    "watch:test": "npm test -- --reporter=min --watch"
  },
  "license": "MIT",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "subject-case": [
        2,
        "always",
        [
          "sentence-case"
        ]
      ]
    }
  },
  "config": {
    "engine-strict": true
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "package.json": "finepack"
  },
  "overrides": {
    "underscore": "^1.13.6"
  }
