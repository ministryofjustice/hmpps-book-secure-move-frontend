{
  "name": "hmpps-book-secure-move-frontend",
  "version": "2.9.0",
  "license": "MIT",
  "description": "Frontend rendering app for the Book a secure move service",
  "homepage": "https://github.com/ministryofjustice/hmpps-book-secure-move-frontend#readme",
  "bugs": {
    "url": "https://github.com/ministryofjustice/hmpps-book-secure-move-frontend/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ministryofjustice/hmpps-book-secure-move-frontend.git"
  },
  "engines": {
    "node": "12"
  },
  "main": "start.js",
  "scripts": {
    "clean": "del .build/",
    "build": "npm run clean && webpack --config webpack.config.js",
    "start": "node start.js",
    "develop": "run-p watch:js:*",
    "test": "NODE_ENV=test LOG_LEVEL=silent mocha './app/**/*.test.js' './common/**/*.test.js' './config/**/*.test.js' --opts ./test/unit/mocha.opts",
    "test-e2e:ci": "npm run test-e2e:chrome -- --color --reporter spec,xunit:reports/testcafe/results-chrome.xml",
    "test-e2e:local": "E2E_BASE_URL='' npm run test-e2e:chrome -- --debug-on-fail",
    "test-e2e:chrome": "testcafe chrome test/e2e/*.test.js",
    "lint": "eslint .",
    "lint-staged": "lint-staged",
    "coverage": "nyc --reporter=html --reporter=lcov npm test",
    "coverage:open": "npm run coverage && open-cli ./coverage/index.html",
    "watch:js:client": "webpack --watch --progress",
    "watch:js:server": "nodemon $NODE_DEBUG_OPTION --inspect",
    "watch:test": "npm test -- --reporter=min --watch",
    "circle:coverage": "nyc --reporter=lcov --reporter=html npm test -- --reporter mocha-junit-reporter --reporter-options mochaFile=reports/mocha/test-results.xml",
    "circle:coveralls": "nyc report --reporter=text-lcov | coveralls",
    "circle:lint": "npm run lint -- --format junit --output-file ./reports/eslint/eslint.xml"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": "eslint"
  },
  "contributors": [
    "Dom Smith <dom@teneightfive.com> (http://github.com/teneightfive)",
    "Steven Lorek <steve@stevelorek.com> (http://github.com/slorek)",
    "Ben Chidgey <ben@feedmypixel.com> (http://github.com/feedmypixel)"
  ],
  "keywords": [
    "ministryofjustice",
    "book-a-secure-move",
    "hmpps"
  ],
  "dependencies": {
    "@ministryofjustice/frontend": "0.0.10-alpha",
    "@sentry/node": "5.6.2",
    "accessible-autocomplete": "1.6.2",
    "axios": "0.19.0",
    "bluebird": "3.5.5",
    "chrono-node": "1.3.11",
    "compression": "1.7.4",
    "connect-flash": "0.1.1",
    "connect-redis": "4.0.2",
    "cookie-parser": "1.4.4",
    "date-fns": "2.3.0",
    "debug": "4.1.1",
    "devour-client": "2.1.0",
    "dotenv": "8.1.0",
    "express": "4.17.1",
    "express-session": "1.16.2",
    "govuk-frontend": "3.2.0",
    "grant-express": "4.6.3",
    "helmet": "3.21.1",
    "hmpo-form-wizard": "11.2.1",
    "i18next": "17.0.16",
    "i18next-express-middleware": "1.8.2",
    "i18next-sync-fs-backend": "1.1.1",
    "json2csv": "4.5.3",
    "lodash": "4.17.15",
    "morgan": "1.9.1",
    "npm-run-all": "4.1.5",
    "nunjucks": "3.2.0",
    "pluralize": "8.0.0",
    "query-string": "6.8.3",
    "redis": "2.8.0",
    "response-time": "2.3.2",
    "serve-favicon": "2.5.0",
    "slashify": "1.0.0",
    "sticky-sidebar": "3.3.1",
    "uuid": "3.3.3",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@babel/preset-env": "7.6.2",
    "@ffmpeg-installer/ffmpeg": "1.0.20",
    "babel-loader": "8.0.6",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "browser-sync": "2.26.7",
    "browser-sync-webpack-plugin": "2.2.2",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "cheerio": "1.0.0-rc.3",
    "coveralls": "3.0.6",
    "css-loader": "3.2.0",
    "del-cli": "3.0.0",
    "eslint": "6.4.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-mocha": "6.1.1",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-prettier": "3.1.1",
