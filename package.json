    "lint-staged": "9.4.2",
    "mini-css-extract-plugin": "0.8.0",
    "mocha": "6.2.0",
    "mocha-junit-reporter": "1.23.1",
    "nock": "11.3.5",
    "node-sass": "4.12.0",
    "nodemon": "1.19.2",
    "nyc": "14.1.1",
    "open-cli": "5.0.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "pre-commit": "1.2.2",
    "prettier": "1.18.2",
    "proxyquire": "2.1.3",
    "sass-loader": "8.0.0",
    "sinon": "7.5.0",
    "sinon-chai": "3.3.0",
    "style-loader": "1.0.0",
    "testcafe": "1.6.0",
    "testcafe-reporter-xunit": "2.1.0",
    "timezone-mock": "1.0.8",
    "uglifyjs-webpack-plugin": "2.2.0",
    "webpack": "4.41.0",
