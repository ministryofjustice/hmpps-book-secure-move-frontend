{% extends "./view.njk" %}

{% block customGtagConfig %}
  gtag('set', {'page_title': 'Move history'});
{% endblock %}

{% block contentMain %}
  {{ tabs(urls, 'timeline') }}
  {% if timeline.items.length %}
    {{ appTimeline(timeline) }}
  {% else %}
    {{ appMessage({
      classes: "app-message--muted",
      allowDismiss: false,
      content: {
        html: t("events::no_events")
      }
    }) }}
  {% endif %}
{% endblock %}
